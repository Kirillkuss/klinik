<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="topicKlinikFirst" class="org.apache.kafka.clients.admin.NewTopic">
        <constructor-arg value="klinikFirst"/>
        <constructor-arg value="1"/>
        <constructor-arg value="1"/>
    </bean>

    <bean id="topicKlinikSecond" class="org.apache.kafka.clients.admin.NewTopic">
        <constructor-arg value="klinikSecond"/>
        <constructor-arg value="1"/>
        <constructor-arg value="1"/>
    </bean>

</beans>